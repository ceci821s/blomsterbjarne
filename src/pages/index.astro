      step4="Hyppighed"
    />
    <ProgressBarAbonnementMobil
      variant="progress_bar_step4"
      step1="Størrelse"
      step2="Stil"
      step3="Duft"
      step4="Hyppighed"
    />
    <ProgressBarKurvMobil
      step1="Kurv"
      step2="Info"
      step3="Betal"
      step4="Bekræft"
    />
    <ProgressBarKurvMobil
      variant="progress_bar_step2"
      step1="Kurv"
      step2="Info"
      step3="Betal"
      step4="Bekræft"
    />
    <ProgressBarKurvMobil
      variant="progress_bar_step3"
      step1="Kurv"
      step2="Info"
      step3="Betal"
      step4="Bekræft"
    />
    <ProgressBarKurvMobil
      variant="progress_bar_step4"
      step1="Kurv"
      step2="Info"
      step3="Betal"
      step4="Bekræft"
    />
    <ProgressBarAbonnementDesktop
      step1="Størrelse"
      step2="Stil"
      step3="Duft"
      step4="Hyppighed"
    />
    <ProgressBarAbonnementDesktop
      variant="progress_bar_step2"
      step1="Størrelse"
      step2="Stil"
      step3="Duft"
      step4="Hyppighed"
    />
    <ProgressBarAbonnementDesktop
      variant="progress_bar_step3"
      step1="Størrelse"
      step2="Stil"
      step3="Duft"
      step4="Hyppighed"
    />
    <ProgressBarAbonnementDesktop
      variant="progress_bar_step4"
      step1="Størrelse"
      step2="Stil"
      step3="Duft"
