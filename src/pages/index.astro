      variant="progress_bar_step4"
      step1="Kurv"
      step2="Info"
      step3="Betal"
      step4="Bekræft"
    />
    <Stepguide
      overskrift="1.stepguide"
      tekst="lorem ipsum"
      progress_variant="progress_bar_card_step_1"
    />
    <Stepguide
      variant="stepguide_desktop"
      overskrift="1.stepguide"
      tekst="lorem ipsum"
      icon="blomst"
    />
