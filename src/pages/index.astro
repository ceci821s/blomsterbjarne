---
import "../style/global.css";

import PrimaryButton from "../components/Primary_button.astro";
import SecondaryButton from "../components/Secondary_button.astro";
import CardAbonnement from "../components/Card_abonnement.astro";
import ProgressBarAbonnementMobil from "../components/Progress_bar_abonnement_mobil.astro";
import ProgressBarAbonnementDesktop from "../components/Progress_bar_abonnement_desktop.astro";
import ProgressBarKurvMobil from "../components/Progress_bar_kurv_mobil.astro";
import ProgressBarKurvDesktop from "../components/Progress_bar_kurv_desktop.astro";
import Stepguide from "../components/Stepguide.astro";
import ProgressBarCard from "../components/Progress_bar_card.astro";
import Footer from "../components/Footer.astro";
import CardSortiment from "../components/Card_sortiment.astro";
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Orienta&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Sansita&display=swap"
      rel="stylesheet"
    />
    <meta name="generator" content={Astro.generator} />
  </head>
  <body>
    <PrimaryButton knaptext="" />
    <PrimaryButton variant="button_primary_desktop" knaptext="" />
    <SecondaryButton knaptext="" />
    <CardAbonnement spand="hej" size="hej" pris="hej" />
    <CardAbonnement
      variant="card_abonnement_desktop"
      spand="hej"
      size="hej"
      pris="hej"
    />
    <ProgressBarAbonnementMobil
      step1="Størrelse"
      step2="Stil"
      step3="Duft"
      step4="Hyppighed"
    />
    <ProgressBarAbonnementMobil
      variant="progress_bar_step2"
      step1="Størrelse"
      step2="Stil"
      step3="Duft"
      step4="Hyppighed"
    />
